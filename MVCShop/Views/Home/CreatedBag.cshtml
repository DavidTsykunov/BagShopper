@model MVCShop.Models.Bag

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CreatedBag</title>
    <link rel="stylesheet" href="~/css/Style.css" />
</head>
<body>
    <h1>
        <a asp-action=""> <img width="100" height="100" src="https://icon-library.com/images/845252_commerce_512x512.png"></a>
        BagShoper
    </h1>

    <table>
        <thead>
            <tr>
                <th><a asp-action=""> Home </a></th>
                <th><a asp-action="AddProduct"> Add Bag </a></th>
                <th><a asp-action="ListBags"> Bags </a></th>
            </tr>
        </thead>
    </table>

    <h1>@Model.Name.ToUpper() has been created! Check Bags to see it on the list!</h1>
</body>
</html>
